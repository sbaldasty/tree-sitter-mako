{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mako",
      "camelcase": "Mako",
      "title": "Mako",
      "scope": "source.mako",
      "file-types": [
        "mako"
      ],
      "injection-regex": "^mako$",
      "class-name": "TreeSitterMako"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Mako grammar for tree-sitter",
    "authors": [
      {
        "name": "Steven Baldasty",
        "email": "sbaldasty@gmail.com",
        "url": "https://bitflippin.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-mako"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}