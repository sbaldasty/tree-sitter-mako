{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mako",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "inherit_block"
          },
          {
            "type": "SYMBOL",
            "name": "injected_html"
          },
          {
            "type": "SYMBOL",
            "name": "module_block"
          },
          {
            "type": "SYMBOL",
            "name": "namespace_block"
          },
          {
            "type": "SYMBOL",
            "name": "python_block"
          }
        ]
      }
    },
    "_ws_opt": {
      "type": "PATTERN",
      "value": "\\s*"
    },
    "_ws_req": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_ws_opt"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_ws_opt"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "_ws_opt"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z_][0-9A-Za-z_]*"
    },
    "inherit_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%inherit"
        },
        {
          "type": "SYMBOL",
          "name": "_ws_req"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "/>"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ">"
                },
                {
                  "type": "STRING",
                  "value": "</%inherit>"
                }
              ]
            }
          ]
        }
      ]
    },
    "module_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%!"
        },
        {
          "type": "SYMBOL",
          "name": "_ws_req"
        },
        {
          "type": "SYMBOL",
          "name": "injected_python_block"
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "namespace_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%namespace"
        },
        {
          "type": "SYMBOL",
          "name": "_ws_req"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "/>"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ">"
                },
                {
                  "type": "STRING",
                  "value": "</%namespace>"
                }
              ]
            }
          ]
        }
      ]
    },
    "python_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%"
        },
        {
          "type": "SYMBOL",
          "name": "_ws_req"
        },
        {
          "type": "SYMBOL",
          "name": "injected_python_block"
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^\"]"
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^']"
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "injected_html"
    },
    {
      "type": "SYMBOL",
      "name": "injected_python_block"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}